<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/account.css" asp-append-version="true"/>
    <title>@ViewData["Title"]</title>
</head>
<body>
<header>
    <partial name="AccountPartial/_AccountNavbarPartial"/>
</header>
<div class="page ms-5 me-5" style="height: 100%">
    <partial name="AccountPartial/_AccountSidebarPartial"/>
    <div class="bg-white" style="overflow-x: auto;">
        <main role="main" class="pb-3">
            <div id="renderBody">@RenderBody()</div>
        </main>
    </div>
</div>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<footer class="bg-black" >
    <partial name="AccountPartial/_AccountFooterPartial"/>
</footer>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
<script src="~/js/consts.js"></script>
<script src="~/js/getToken.js"></script>
<script src="~/js/getClaims.js"></script>
<script src="~/js/account/premiums.js"></script>
</body>
</html>